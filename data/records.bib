   colrev.semantic_scholar.id    = {https://www.semanticscholar.org/paper/fbeb4b2b03c5c1be2b96a6a976fb749c83fa68ec},
   author                        = {Jiang, James J. and Klein, Gary and Means, Tom},
   journal                       = {Information Systems Journal},
   title                         = {The missing link between systems analysts' actions and skills},
   year                          = {1999},
   volume                        = {9},
   number                        = {1},
   pages                         = {21--33},
   abstract                      = {System analysts approach tasks with different orientations to their actions. Likewise, they exhibit a variety of skills. A study is conducted to explore the links, or similarities, between these orientations and skills. The findings indicate that analysts do indeed have orientations corresponding to their skills. This trait enables information systems management to select project teams appropriate to the development context. The variety of skills and orientations needed for success can be chosen based on one trait. Training of analysts can be targeted more effectively to consider missing skills based on current orientations.},
   language                      = {eng},
}


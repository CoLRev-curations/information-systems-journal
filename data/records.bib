   colrev_pdf_id                 = {cpid2:ffffffe0e0e6ffffefffffc7e03fffffed9607ffe00000cfc0000007e00007ffffffffffe0c0ffffe017ffffffffffffe3f8ffffe0f80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fff80007fffc7fffe0000fffe07ffff7ffffffff},
   file                          = {data/pdfs/35_4/HekkalaNordback2025.pdf},
   doi                           = {10.1111/ISJ.12569},
   colrev.dblp.dblp_key          = {https://dblp.org/rec/journals/isj/HekkalaN25},
   author                        = {Hekkala, Riitta and Nordbäck, Emma},
   journal                       = {Information Systems Journal},
   title                         = {Unpacking reflexivity, psychological empowerment, and agile project dynamics among information systems professionals},
   year                          = {2025},
   volume                        = {35},
   number                        = {4},
   pages                         = {1101--1131},
   abstract                      = {In this study, we examine the reflexivity modes of information systems (IS) professionals and integrate the IS context—including structural, technological, and social properties—with IS professionals' biographical, inner dialogues to theorise about the role of reflexivity modes in their experiences of psychological empowerment. Through the lens of Archer's (2007) three‐stage model of reflexivity and different modes of reflexivity (communicative, autonomous, meta, and fractured), we investigate the experiences of psychological empowerment and project dynamics among IS professionals in IS development work by zooming in on 6 years of three IS professionals' work lives. Our findings make several contributions to theory and practice. First, we elucidate the interplay between reflexivity and psychological empowerment. Second, we show how different IS professionals' approach similar structural, technological, or social circumstances differently (such as agile work practices), depending on their reflexive inner dialogues. This includes showing how different reflexivity modes, focused on various properties of the IS context (structural, technological, and social), becomes entangled with interpersonal dynamics and IS project dynamics.},
   language                      = {eng},
}


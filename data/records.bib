@article{HaoFengKamoche2025,
   colrev_origin                 = {CROSSREF.bib/001112;
                                    pdfs.bib/000945;},
   colrev_status                 = {rev_included},
   colrev_masterdata_provenance  = {author:pdfs.bib/000945;;
                                    journal:pdfs.bib/000945;;

@article{NordbottenCrosby1999,
   colrev_origin                 = {CROSSREF.bib/NordbottenCrosby1999;
                                    DBLP.bib/000168;
                                    pdfs.bib/0000000544;},
   colrev_status                 = {md_processed},
   colrev_masterdata_provenance  = {CURATED:https://github.com/CoLRev-curations/information-systems-journal;;},
   colrev_data_provenance        = {file:merged;;
                                    sem_scholar_id:https://api.semanticscholar.org/v1/paper/2b951f3834dd68c3a0e05600320ea8edf2041180;;
                                    abstract:https://api.semanticscholar.org/v1/paper/2b951f3834dd68c3a0e05600320ea8edf2041180;;
                                    language:http://dx.doi.org/10.1046/J.1365-2575.1999.00052.X;;},
   colrev_id                     = {colrev_id1:|a|information-systems-journal|9|2|1999|nordbotten-crosby|the-effect-of-graphic-style-on-data-model-interpretation;},
   file                          = {data/pdfs/9_2/Nordbotten_The Effect Of Graph.pdf},
   doi                           = {10.1046/J.1365-2575.1999.00052.X},
   dblp_key                      = {https://dblp.org/rec/journals/isj/NordbottenC99},
   sem_scholar_id                = {https://www.semanticscholar.org/paper/2b951f3834dd68c3a0e05600320ea8edf2041180},
   author                        = {Nordbotten, Joan C. and Crosby, Martha E.},
   journal                       = {Information Systems Journal},
   title                         = {The effect of graphic style on data model interpretation},
   year                          = {1999},
   volume                        = {9},
   number                        = {2},
   pages                         = {139--155},
   abstract                      = {Abstract. Graphic data models are commonly used as a tool for presentation of information structures in the design, implementation, use and maintenance of the databases that support information systems. The methods proposed for database design assume that the use of graphic data models will enhance understanding of system specifications by both the end‐users and the implementers of the system. For this assumption to hold, the information presented in the graphic data model must be readily comprehensible so that the design, represented by the model, can be confirmed and implemented correctly. The lack of standard representations for graphic models has led to a variety of graphic styles. To date, there has been little focus on studying the effect graphic style has on model comprehension. We have studied the effect of three graphic styles proposed for data models on model legibility and interpretation. Our study shows a significant variation in model interpretation that can be attributed to the graphic syntax used. Graphic style appears to influence which model elements are included in the interpretation, as well as the way data models are read.},
   language                      = {eng},
}


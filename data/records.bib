@article{WimeliusMathiassenHolmstromEtAl2021,
   colrev_origin                 = {CROSSREF.bib/WimeliusMathiassenHolmstromEtAl2021;
                                    DBLP.bib/000770;
                                    pdfs.bib/0000000728;},
   colrev_status                 = {md_processed},

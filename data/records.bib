   colrev_pdf_id                 = {cpid2:ffffffe0e0f35fffefffffc7e03fffffef66c9ffe00000ffe00007fffffffbffc060c3ffffffffffe778ffffe0780007e3f80007e0780007e0380007e3f80007e0380007e9f80007e1380007fff80007fff80007fff80007fff80007fff80007fff80007fff80e17fff80007ffffffffe000001fe0000fffe80fffc7ffffffff},

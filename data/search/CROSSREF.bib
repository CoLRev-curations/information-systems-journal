   doi                           = {10.1111/ISJ.12591},
   author                        = {Davison, Robert M. and Schwabe, Gerhard},
   journal                       = {Information Systems Journal},
   title                         = {The Future (As a Focus) of {IS} Research},
   year                          = {2025},
   volume                        = {35},
   number                        = {6},
   pages                         = {1513--1515},
   language                      = {eng},

   author                        = {Zamani, Efpraxia D. and Watson‐Manheim, Mary Beth and Abbott, Pamela and Lin, Angela},
   journal                       = {Information Systems Journal},
   title                         = {The new wave of 'hybrid' work: An opportunity to revise assumptions and build theory},
   year                          = {2024},
